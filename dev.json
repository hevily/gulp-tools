{
    "pkg": "pro",
    "version": "1.0",
    "meta": {
        "devPath": "build/",
        "outPath": "dist/",
        "imgPath": "img/",
        "lessPath": "less/",
        "cssPath": "css/",
        "jsPath": "js/",
        "libPath": "lib/"
    },
    "devSuffix":"_",
    "outSuffix":".min",
    "except":["require" ,"exports" ,"module" ,"$", "mui","Vue","VueMaterial"],
    "browsers":["> 1%","last 5 versions", "Android >= 4.0","ios 7"],
    "fileinclude":{
        "prefix": "@@",
        "suffix": ""
    },
    "jsfileinclude":{
        "prefix": "//{{",
        "suffix": "}}"
    }
}